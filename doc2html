#!/usr/bin/env php

<?php

use Tamdaz\Doc2Html\ClassReflector;
use Tamdaz\Doc2Html\ObjectRegister;

require_once "vendor/autoload.php";

$objectRegister = ObjectRegister::getInstance();

$objectRegister->registerNamespace("Examples");
$objectRegister->registerNamespace("Tamdaz\Doc2Html");

$reflector = new ClassReflector();

$reflector->run();